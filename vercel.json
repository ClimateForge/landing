{
    "rewrites": [
        {
            "source": "/",
            "destination": "https://climateforge.vercel.app/",
            "has": [
                {
                    "type": "host",
                    "value": "climateforge.vercel.app"
                }
            ]
        },
        {
            "source": "/:match*",
            "destination": "https://climateforge.vercel.app/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "climateforge.vercel.app"
                }
            ]
        },
        {
            "source": "/",
            "destination": "https://climateforge.ai/",
            "has": [
                {
                    "type": "host",
                    "value": "climateforge.ai"
                }
            ]
        },
        {
            "source": "/:match*",
            "destination": "https://climateforge.ai/:match*",
            "has": [
                {
                    "type": "host",
                    "value": "climateforge.ai"
                }
            ]
        }
    ]
}